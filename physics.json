{
  "categorias": [
    {
      "nombre": "Kinematics",
      "id": "cinematica",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "v = \\frac{\\Delta x}{\\Delta t}",
          "title": "Average velocity"
        },
        {
          "type": "button",
          "latex": "a = \\frac{\\Delta v}{\\Delta t}",
          "title": "Average acceleration"
        },
        {
          "type": "button",
          "latex": "x = x_0 + v t",
          "title": "Uniform motion"
        },
        {
          "type": "button",
          "latex": "x = x_0 + v_0 t + \\frac{1}{2} a t^2",
          "title": "Position with constant acceleration"
        },
        {
          "type": "button",
          "latex": "v = v_0 + a t",
          "title": "Velocity with constant acceleration"
        },
        {
          "type": "button",
          "latex": "v^2 = v_0^2 + 2 a (x - x_0)",
          "title": "Velocity without time"
        }
      ]
    },
    {
      "nombre": "Dynamics",
      "id": "dinamica",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "F = m a",
          "title": "Newton's second law"
        },
        {
          "type": "button",
          "latex": "p = m v",
          "title": "Linear momentum"
        },
        {
          "type": "button",
          "latex": "F = \\frac{\\Delta p}{\\Delta t}",
          "title": "Force and impulse"
        },
        {
          "type": "button",
          "latex": "W = F d \\cos\\theta",
          "title": "Work"
        },
        {
          "type": "button",
          "latex": "P = \\frac{W}{t}",
          "title": "Power"
        }
      ]
    },
    {
      "nombre": "Energy and Work",
      "id": "energia",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "E_c = \\frac{1}{2} m v^2",
          "title": "Kinetic energy"
        },
        {
          "type": "button",
          "latex": "E_p = m g h",
          "title": "Gravitational potential energy"
        },
        {
          "type": "button",
          "latex": "E_m = E_c + E_p",
          "title": "Mechanical energy"
        },
        {
          "type": "button",
          "latex": "\\Delta E = W",
          "title": "Work-energy theorem"
        },
        {
          "type": "button",
          "latex": "P = V I",
          "title": "Electric power"
        }
      ]
    },
    {
      "nombre": "Gravitation",
      "id": "gravitacion",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "F = G \\frac{m_1 m_2}{r^2}",
          "title": "Law of universal gravitation"
        },
        {
          "type": "button",
          "latex": "g = \\frac{G M}{r^2}",
          "title": "Gravitational field"
        },
        {
          "type": "button",
          "latex": "E_p = -G \\frac{m M}{r}",
          "title": "Gravitational potential energy"
        },
        {
          "type": "button",
          "latex": "v = \\sqrt{\\frac{G M}{r}}",
          "title": "Orbital velocity"
        }
      ]
    },
    {
      "nombre": "Waves and Oscillations",
      "id": "ondas_oscilaciones",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "v = \\lambda f",
          "title": "Wave speed"
        },
        {
          "type": "button",
          "latex": "T = \\frac{1}{f}",
          "title": "Period"
        },
        {
          "type": "button",
          "latex": "f = \\frac{1}{T}",
          "title": "Frequency"
        },
        {
          "type": "button",
          "latex": "F = -k x",
          "title": "Hooke's law"
        },
        {
          "type": "button",
          "latex": "T = 2\\pi \\sqrt{\\frac{m}{k}}",
          "title": "SHM Period"
        },
        {
          "type": "button",
          "latex": "x(t) = A \\cos(\\omega t + \\phi)",
          "title": "Simple harmonic motion"
        }
      ]
    },
    {
      "nombre": "Electricity and Magnetism",
      "id": "electromagnetismo",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "V = I R",
          "title": "Ohm's law"
        },
        {
          "type": "button",
          "latex": "R_{eq} = R_1 + R_2",
          "title": "Resistors in series"
        },
        {
          "type": "button",
          "latex": "\\frac{1}{R_{eq}} = \\frac{1}{R_1} + \\frac{1}{R_2}",
          "title": "Resistors in parallel"
        },
        {
          "type": "button",
          "latex": "C = \\frac{Q}{V}",
          "title": "Capacitance"
        },
        {
          "type": "button",
          "latex": "U = \\tfrac{1}{2} C V^2",
          "title": "Energy in capacitor"
        },
        {
          "type": "button",
          "latex": "q\\,v\\,B\\sin\\theta",
          "title": "Lorentz force"
        },
        {
          "type": "button",
          "latex": "\\Phi = B A \\cos\\theta",
          "title": "Magnetic flux"
        },
        {
          "type": "button",
          "latex": "\\mathcal{E} = -\\frac{d\\Phi}{dt}",
          "title": "Faraday's law"
        }
      ]
    },
    {
      "nombre": "Optics",
      "id": "optica",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "n_1 \\sin\\theta_1 = n_2 \\sin\\theta_2",
          "title": "Snell's law"
        },
        {
          "type": "button",
          "latex": "\\frac{1}{f} = \\frac{1}{d_o} + \\frac{1}{d_i}",
          "title": "Thin lens equation"
        },
        {
          "type": "button",
          "latex": "M = -\\frac{d_i}{d_o}",
          "title": "Lateral magnification"
        },
        {
          "type": "button",
          "latex": "\\theta_i = \\theta_r",
          "title": "Law of reflection"
        }
      ]
    },
    {
      "nombre": "Fluids",
      "id": "fluidos",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "P = \\frac{F}{A}",
          "title": "Pressure"
        },
        {
          "type": "button",
          "latex": "P = P_0 + \\rho g h",
          "title": "Hydrostatic pressure"
        },
        {
          "type": "button",
          "latex": "A_1 v_1 = A_2 v_2",
          "title": "Continuity equation"
        },
        {
          "type": "button",
          "latex": "P + \\tfrac12 \\rho v^2 + \\rho g h = \\text{constant}",
          "title": "Bernoulli's equation"
        }
      ]
    },
    {
      "nombre": "Thermodynamics",
      "id": "termodinamica",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "PV = n R T",
          "title": "Ideal gas law"
        },
        {
          "type": "button",
          "latex": "Q = m c \\Delta T",
          "title": "Sensible heat"
        },
        {
          "type": "button",
          "latex": "Q = m L",
          "title": "Latent heat"
        },
        {
          "type": "button",
          "latex": "\\Delta U = Q - W",
          "title": "First law of thermodynamics"
        },
        {
          "type": "button",
          "latex": "\\Delta S = \\frac{Q_{rev}}{T}",
          "title": "Entropy"
        }
      ]
    }
  ]
}
