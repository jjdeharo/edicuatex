{
  "categorias": [
    {
      "nombre": "Chemical symbolic notation",
      "id": "notacion-quimica",
      "grid_template_columns": "repeat(auto-fit, minmax(120px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "_{}^{}\\mathrm{}",
          "display": "_{6}^{12}\\mathrm{C}",
          "title": "Element with atomic and mass number"
        },
        {
          "type": "button",
          "latex": "\\ce{}",
          "display": "\\ce{H2O}",
          "title": "Chemical formula (mhchem package)"
        },
        {
          "type": "button",
          "latex": "\\ce{->}",
          "display": "\\ce{->}",
          "title": "Reaction arrow"
        },
        {
          "type": "button",
          "latex": "\\ce{<-}",
          "display": "\\ce{<-}",
          "title": "Reverse reaction arrow"
        },
        {
          "type": "button",
          "latex": "\\ce{<=>}",
          "display": "\\ce{<=>}",
          "title": "Reversible reaction"
        },
        {
          "type": "button",
          "latex": "\\ce{CO2 + H2O -> H2CO3}",
          "display": "\\ce{CO2 + H2O -> H2CO3}",
          "title": "Example of chemical reaction"
        },
        {
          "type": "button",
          "latex": "\\ce{^{23}_{11}Na^+}\n",
          "display": "",
          "title": "Positive ion"
        },
        {
          "type": "button",
          "latex": "\\ce{Cl^-}",
          "display": "\\ce{Cl^-}",
          "title": "Negative ion"
        },
        {
          "type": "button",
          "latex": "\\ce{Ag_{(s)}}",
          "display": "\\ce{Ag_{(s)}}",
          "title": "Solid state"
        },
        {
          "type": "button",
          "latex": "\\ce{H2O_{(l)}}",
          "display": "\\ce{H2O_{(l)}}",
          "title": "Liquid state"
        },
        {
          "type": "button",
          "latex": "\\ce{CO2_{(g)}}",
          "display": "\\ce{CO2_{(g)}}",
          "title": "Gaseous state"
        },
        {
          "type": "button",
          "latex": "\\ce{NaCl_{(aq)}}",
          "display": "\\ce{NaCl_{(aq)}}",
          "title": "Aqueous state"
        },
        {
          "type": "button",
          "latex": "\\ce{2H2 + O2 -> 2H2O}",
          "display": "\\ce{2H2 + O2 -> 2H2O}",
          "title": "Balanced reaction"
        }
      ]
    },
    {
      "nombre": "Ideal gases",
      "id": "gases_ideales",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "PV = n R T",
          "title": "Ideal gas law equation"
        },
        {
          "type": "button",
          "latex": "d = \\frac{PM}{RT}",
          "title": "Density of an ideal gas"
        },
        {
          "type": "button",
          "latex": "p_i = x_i P",
          "title": "Partial pressure"
        }
      ]
    },
    {
      "nombre": "Stoichiometry",
      "id": "estequiometria",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "n = \\frac{m}{M}",
          "title": "Amount of substance"
        },
        {
          "type": "button",
          "latex": "m = n M",
          "title": "Mass of substance"
        },
        {
          "type": "button",
          "latex": "C = \\frac{n}{V}",
          "title": "Molar concentration"
        },
        {
          "type": "button",
          "latex": "C_1 V_1 = C_2 V_2",
          "title": "Dilution"
        }
      ]
    },
    {
      "nombre": "Chemical equilibrium",
      "id": "equilibrio_quimico",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "K_c = \\frac{[C]^c [D]^d}{[A]^a [B]^b}",
          "title": "Equilibrium constant (Kc)"
        },
        {
          "type": "button",
          "latex": "K_p = K_c (RT)^{\\Delta n}",
          "title": "Equilibrium constant (Kp)"
        },
        {
          "type": "button",
          "latex": "Q = \\frac{[C]^c [D]^d}{[A]^a [B]^b}",
          "title": "Reaction quotient"
        }
      ]
    },
    {
      "nombre": "Acid-base",
      "id": "acido_base",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "pH = -\\log_{10}[H^+]",
          "title": "pH"
        },
        {
          "type": "button",
          "latex": "pOH = -\\log_{10}[OH^-]",
          "title": "pOH"
        },
        {
          "type": "button",
          "latex": "pH + pOH = 14",
          "title": "pH-pOH relationship"
        },
        {
          "type": "button",
          "latex": "K_a = \\frac{[H^+][A^-]}{[HA]}",
          "title": "Acidity constant"
        },
        {
          "type": "button",
          "latex": "pK_a = -\\log_{10}K_a",
          "title": "pKa"
        },
        {
          "type": "button",
          "latex": "pH = pK_a + \\log_{10}\\frac{[A^-]}{[HA]}",
          "title": "Hendersonâ€“Hasselbalch"
        }
      ]
    },
    {
      "nombre": "Thermochemistry",
      "id": "termoquimica",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "Q = m c \\Delta T",
          "title": "Sensible heat"
        },
        {
          "type": "button",
          "latex": "Q = n \\Delta H_{\\text{rxn}}",
          "title": "Heat of reaction"
        },
        {
          "type": "button",
          "latex": "\\Delta G = \\Delta H - T \\Delta S",
          "title": "Gibbs free energy"
        }
      ]
    },
    {
      "nombre": "Constants",
      "id": "constantes_quimicas",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "N_A = 6.022\\times10^{23}\\,\\mathrm{mol}^{-1}",
          "title": "Avogadro's number"
        },
        {
          "type": "button",
          "latex": "R = 8.314\\,\\mathrm{J mol^{-1}K^{-1}}",
          "title": "Gas constant"
        },
        {
          "type": "button",
          "latex": "k_B = 1.381\\times10^{-23}\\,\\mathrm{J/K}",
          "title": "Boltzmann constant"
        }
      ]
    }
  ]
}
